<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="5.0" prefHeight="200.0" prefWidth="600.0" vgap="5.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="airplaneticketbooking.PassportCheckController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Label text="Verify your passport ID :">
         <font>
            <Font name="Times New Roman" size="25.0" />
         </font>
      </Label>
      <TextField fx:id="passportIDVerify" onAction="#verify" prefHeight="35.0" prefWidth="240.0" promptText="enter your passport ID" GridPane.columnIndex="1">
         <font>
            <Font name="Times New Roman" size="20.0" />
         </font>
      </TextField>
      <Button fx:id="confirmBtn" mnemonicParsing="false" onAction="#verify" prefHeight="35.0" prefWidth="100.0" text="Confirm" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.rowIndex="1">
         <font>
            <Font name="Times New Roman" size="20.0" />
         </font>
         <GridPane.margin>
            <Insets />
         </GridPane.margin>
      </Button>
      <Button fx:id="cancelBtn" mnemonicParsing="false" onAction="#cancel" prefHeight="35.0" prefWidth="100.0" text="Cancel" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="1">
         <font>
            <Font name="Times New Roman" size="20.0" />
         </font>
      </Button>
   </children>
</GridPane>
